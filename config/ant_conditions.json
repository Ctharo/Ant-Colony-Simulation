{
  "conditions": {
	"AtHome": {
	  "description": "Check if ant within boundary of colony radius",
	  "evaluation": {
		"operator": "LESS_THAN",
		"property": "proprioception.colony.distance",
		"type": "PropertyCheck",
		"value_from": "colony.radius"
	  }
	},
	"IsCarryingFood": {
	  "description": "Check if ant is carrying any food",
	  "evaluation": {
		"operator": "EQUALS",
		"property": "storage.status.is_carrying",
		"type": "PropertyCheck",
		"value": true
	  }
	},
	"IsFoodInReach": {
	  "description": "Check if food is reachable by ant",
	  "evaluation": {
		"operator": "GREATER_THAN",
		"property": "reach.foods.count",
		"type": "PropertyCheck",
		"value": 0
	  }
	},
	"IsFoodInView": {
	  "description": "Check if food is visible to ant",
	  "evaluation": {
		"operator": "GREATER_THAN",
		"property": "vision.foods.count",
		"type": "PropertyCheck",
		"value": 0
	  }
	},
	"IsFoodPheromoneSensed": {
	  "description": "Check if food pheromones are detected",
	  "evaluation": {
		"operator": "EQUALS",
		"property": "olfaction.pheromones.food.status.is_detecting",
		"type": "PropertyCheck",
		"value": true
	  }
	},
	"IsHomePheromoneSensed": {
	  "description": "Check if home pheromones are detected",
	  "evaluation": {
		"operator": "EQUALS",
		"property": "olfaction.pheromones.home.status.is_detecting",
		"type": "PropertyCheck",
		"value": true
	  }
	},
	"IsAnyPheromoneSensed": {
	  "description": "Check if any pheromones are detected",
	  "evaluation": {
		"operator": "EQUALS",
		"property": "olfaction.pheromones.status.is_detecting",
		"type": "PropertyCheck",
		"value": true
	  }
	},
	"LowEnergy": {
	  "description": "Check if ant's energy is low but not depleted",
	  "evaluation": {
		"operator": "LESS_THAN",
		"property": "energy.capacity.percentage",
		"type": "PropertyCheck",
		"value": 20
	  }
	},
	"HasStorageSpace": {
	  "description": "Check if ant has space to store more items",
	  "evaluation": {
		"operator": "NOT_EQUALS",
		"property": "storage.status.is_full",
		"type": "PropertyCheck",
		"value": true
	  }
	},
	"CanMoveAndAct": {
	  "description": "Check if ant is able to move and perform actions",
	  "evaluation": {
		"type": "Operator",
		"operator_type": "and",
		"operands": [
		  {
			"type": "Operator",
			"operator_type": "not",
			"operands": [
			  {
				"type": "PropertyCheck",
				"property": "strength.status.overloaded",
				"operator": "EQUALS",
				"value": true
			  }
			]
		  },
		  {
			"type": "Operator",
			"operator_type": "not",
			"operands": [
			  {
				"type": "PropertyCheck",
				"property": "energy.status.is_depleted",
				"operator": "EQUALS",
				"value": true
			  }
			]
		  }
		]
	  }
	}
  }
}
